<div class="eventhead">
  <h2 class="event">
    {{> meetupDate }}
    {{ name }}
  </h2>
  <ul>  
  {{#each calls.0.submissions}}
  <li class="speaker">
    <img src='{{#gravatar email size="64"}}{{/gravatar}}'>
    <div class="title">{{ title }}</div><div class="presented">Presented by {{ name }}</div>
  </li>
  {{/each }}
  </ul>
</div>
